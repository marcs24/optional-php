{
    "name": "optional/optional-php",
    "description": "wrapper to comfortable access on arrays/object attributes and methods that may not exist",
    "license": "MIT",
    "type": "library",
    "keywords": ["optional","stablecode", "utils"],
    "version": "0.1.0",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "phpstan/phpstan": "^0.12.0"
    },
    "autoload": {
        "psr-4": {
            "Utils\\": "src/",
            "Utils\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse src tests",
        "test-cov": "phpunit --coverage-html coverage-html"
    }
}
